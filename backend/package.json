{
  "name": "crescite-backend",
  "version": "1.0.0",
  "description": "Crescite - Automated Financial Portfolio Optimizer Backend",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec tsx src/server.ts",
    "dev:worker": "nodemon --exec tsx src/workers/worker-manager.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "start:worker": "node dist/workers/worker-manager.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "check": "tsc --noEmit"
  },
  "keywords": ["portfolio", "finance", "optimization"],
  "author": "Crescite Team",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.470.0",
    "@aws-sdk/s3-request-presigner": "^3.470.0",
    "@prisma/client": "^5.7.0",
    "amqplib": "^0.10.3",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "pdf-parse": "^1.1.1",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.4",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.5",
    "@types/pdf-parse": "^1.1.4",
    "@types/uuid": "^9.0.7",
    "nodemon": "^3.0.2",
    "prisma": "^5.7.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
